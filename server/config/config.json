{
  "database": {
    "uri": "mongodb://127.0.0.1:27017/user_db",
    "options": {
      "useMongoClient": true
    }
  },
  "log": {
    "appenders": {
      "out": {
        "type": "stdout",
        "layout": {
          "type": "basic"
        }
      },
      "app": {
        "type": "dateFile",
        "filename": "./log/app.log",
        "pattern": ".yyyy-MM-dd",
        "alwaysIncludePattern": true,
        "layout": {
          "type": "basic"
        },
        "daysToKeep": 7,
        "compress": true
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "out",
          "app"
        ],
        "level": "debug"
      }
    }
  },
  "jwt": {
    "secretKey": "hqF65AiumUo=",
    "tokenExpires": "1 days"
  }
}
